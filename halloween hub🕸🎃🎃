local Troll = MakeTab({Name = "troll"})

local playerList = {}
local players = game:GetService("Players"):GetPlayers()
for _, player in pairs(players) do
  table.insert(playerList, player.Name)
end

local selectedPlayer = nil
local isViewing = false
local camera = game.Workspace.CurrentCamera

AddDropdown(Troll, {
  Name = "Target",
  Options = playerList,
  Callback = function(selected)
    selectedPlayer = selected
  end
})

AddButton(Troll, {
  Name = "Goto",
  Callback = function()
    if selectedPlayer then
      local targetPlayer = game:GetService("Players"):FindFirstChild(selectedPlayer)
      if targetPlayer then
        local character = targetPlayer.Character
        if character then
          local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
          if humanoidRootPart then
            game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = humanoidRootPart.CFrame
          end
        end
      end
    end
  end
})

AddButton(Troll, {
  Name = "Bring",
  Callback = function()
    local args = {
      [1] = "PickingTools",
      [2] = "Couch"
    }

    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
    
    local player = game.Players.LocalPlayer

    local function equiparTools()
      local tools = player.Backpack:GetChildren()

      for _, tool in ipairs(tools) do
        if tool:IsA("Tool") then
          player.Character.Humanoid:EquipTool(tool)
        end
      end
    end

    equiparTools()

    local tempoLimite = 1
    local inicioTempo = tick()

    while tick() - inicioTempo < tempoLimite do
      equiparTools()
      wait(0.1)
    end
    
    if selectedPlayer then
      local targetPlayer = game:GetService("Players"):FindFirstChild(selectedPlayer)
      if targetPlayer then
        local character = targetPlayer.Character
        if character then
          local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
          if humanoidRootPart then
            local player = game.Players.LocalPlayer
            local originalPosition = player.Character:FindFirstChild("HumanoidRootPart").CFrame
            
            local endTime = tick() + 5
            while tick() < endTime do
              player.Character:FindFirstChild("HumanoidRootPart").CFrame = humanoidRootPart.CFrame
              wait(0.1)
            end

            player.Character:FindFirstChild("HumanoidRootPart").CFrame = originalPosition
          end
        end
      end
    end
  end
})

AddToggle(Troll, {
  Name = "View",
  Callback = function(state)
    isViewing = state
    if isViewing and selectedPlayer then
      local targetPlayer = game:GetService("Players"):FindFirstChild(selectedPlayer)
      if targetPlayer then
        local character = targetPlayer.Character
        if character then
          local targetCamera = character:FindFirstChild("HumanoidRootPart")
          if targetCamera then
            camera.CameraSubject = targetCamera
            camera.CameraType = Enum.CameraType.Custom
          end
        end
      end
    else
      camera.CameraSubject = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
      camera.CameraType = Enum.CameraType.Custom
    end
  end
})

local Main = MakeTab({Name = "avatar"})

AddButton(Main, {
  Name = "fire avatar(visual)",
  Callback = function()
    local Players = game:GetService("Players")
    local Lighting = game:GetService("Lighting")

    local function createFireOnCharacter(character)
      local fire = Instance.new("Fire")
      fire.Name = "CharacterFire"
      fire.Size = 15
      fire.Heat = 10
      fire.Color = Color3.fromRGB(255, 165, 0)
      
      for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
          local fireClone = fire:Clone()
          fireClone.Parent = part
        end
      end
    end

    local function hideFireForOthers()
      local localPlayer = Players.LocalPlayer
      
      if localPlayer and localPlayer.Character then
        createFireOnCharacter(localPlayer.Character)
        
        for _, player in pairs(Players:GetPlayers()) do
          if player ~= localPlayer then
            player.PlayerGui:SetCore("ChatMakeSystemMessage", {
              Text = "O fogo est√° oculto para voc√™.",
              Color = Color3.fromRGB(255, 165, 0),
              Font = Enum.Font.SourceSans,
              TextSize = 24
            })
          end
        end
        
        wait(6)
        
        for _, part in pairs(localPlayer.Character:GetDescendants()) do
          if part:IsA("Fire") then
            part:Destroy()
          end
        end
      end
    end

    hideFireForOthers()
  end
})

AddButton(Main, {
  Name = "gigante(visual)",
  Callback = function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    local scaleFactor = 1.4

    local function scaleCharacter()
      humanoid.BodyHeightScale.Value = scaleFactor
      humanoid.BodyWidthScale.Value = scaleFactor
      humanoid.BodyDepthScale.Value = scaleFactor

      for _, part in ipairs(character:GetChildren()) do
        if part:IsA("Part") or part:IsA("MeshPart") then
          part.Size = part.Size * scaleFactor
          part.CanCollide = false
        end
      end
    end

    scaleCharacter()
  end
})