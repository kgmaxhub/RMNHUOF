local Main = MakeTab({Name = "avatar"})

local section = AddSection(Main, {"Fogo Player"})

AddButton(Main, {
  Name = "Fire avatar",
  Callback = function()
    local Players = game:GetService("Players")
    local Lighting = game:GetService("Lighting")

    local function createFireOnCharacter(character)
      local fire = Instance.new("Fire")
      fire.Name = "CharacterFire"
      fire.Size = 15
      fire.Heat = 10
      fire.Color = Color3.fromRGB(255, 165, 0)
      
      for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
          local fireClone = fire:Clone()
          fireClone.Parent = part
        end
      end
    end

    local function hideFireForOthers()
      local localPlayer = Players.LocalPlayer
      
      if localPlayer and localPlayer.Character then
        createFireOnCharacter(localPlayer.Character)
        
        for _, player in pairs(Players:GetPlayers()) do
          if player ~= localPlayer then
            player.PlayerGui:SetCore("ChatMakeSystemMessage", {
              Text = "O fogo est√° oculto para voc√™.",
              Color = Color3.fromRGB(255, 165, 0),
              Font = Enum.Font.SourceSans,
              TextSize = 24
            })
          end
        end
        
        wait(6)
        
        for _, part in pairs(localPlayer.Character:GetDescendants()) do
          if part:IsA("Fire") then
            part:Destroy()
          end
        end
      end
    end

    hideFireForOthers()
  end
})

local section = AddSection(Main, {"Skin Rainbow"})

-- Lista de cores para alternar
local colors = {
    "Earth green",
    "Magenta",
    "Maroon",
    "Toothpaste",
    "Institutional white"
}

-- Vari√É¬°vel para controlar o estado do toggle
local isAutoColorEnabled = false

-- Fun√É¬ß√É¬£o para alternar automaticamente as cores
local function AutoColorToggle(toggleValue)
    isAutoColorEnabled = toggleValue  -- Atualiza o estado do toggle

    -- Se o toggle for desativado, n√É¬£o faz mais nada
    if not toggleValue then
        return
    end

    -- Loop para alternar entre as cores
    local index = 1
    while isAutoColorEnabled do
        local args = {
            [1] = "skintone",
            [2] = colors[index]
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Updat1eAvata1r"):FireServer(unpack(args))
        index = index % #colors + 1  -- Avan√É¬ßa para a pr√É¬≥xima cor circularmente
        wait(2)  -- Espera 3 segundos antes de trocar para a pr√É¬≥xima cor (ajuste conforme necess√É¬°rio)
    end
end

-- Exemplo de uso:
AddToggle(Main, {
    Name = "Rainbow Skin",
    Default = false,
    Callback = function(Value)
        AutoColorToggle(Value)
    end
})

local section = AddSection(Main, {"Ficar gigante"})

AddButton(Main, {
  Name = "Gigante",
  Callback = function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    local scaleFactor = 1.4

    local function scaleCharacter()
      humanoid.BodyHeightScale.Value = scaleFactor
      humanoid.BodyWidthScale.Value = scaleFactor
      humanoid.BodyDepthScale.Value = scaleFactor

      for _, part in ipairs(character:GetChildren()) do
        if part:IsA("Part") or part:IsA("MeshPart") then
          part.Size = part.Size * scaleFactor
          part.CanCollide = false
        end
      end
    end

    scaleCharacter()
  end
})

